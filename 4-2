#include <iostream>

using namespace std;

int main() {
    float a, b, c;

    cout << "Enter 3 values: ";
    cin >> a >> b >> c;

    if(a > b && b > c )
      cout << c << " " << b << " " << a;
    else if(a > c && c > b)
      cout << b << " " << c << " " << a;
    else if(b > a && a > c)
      cout << c << " " << b << " " << a;
    else if(b > c && c > a)
      cout << a << " " << b << " " << a;
    else if(c > a && a > b)
      cout << b << " " << a << " " << c;
    else
      cout << a << " " << b << " " << c;
    
    system("PAUSE");
    return 0;
}
